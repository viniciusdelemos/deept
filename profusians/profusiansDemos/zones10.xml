<zone shape="rectangular" itemtype="flexible" name="green zone" shapedata="100,-100,140,140"  itemcolor="0,200,0" fillcolor="0,200,0,111"></zone>
<zone shape="rectangular" itemtype="flexible" name="purple zone" shapedata="-100,100,120,160"  itemcolor="100,0,100" fillcolor="100,0,100,111"></zone>



</zonexml>
